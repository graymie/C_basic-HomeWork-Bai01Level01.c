#include "stdio.h"
#include"conio.h"
#include"math.h"
int main()
{
	int a,d;
	
	printf("nhap so ngay");
	scanf("%d",&a);
	d= a/30;
	if (a>=30)
	{
	if (d>=12)
	printf(" \n %d nam,%d thang,%d ngay",d/12,d%12,a%30);
	else printf(" %d thang,%d ngay",d, a%30);
	}
	else printf("%d ngay",a);
	
	getch();}
